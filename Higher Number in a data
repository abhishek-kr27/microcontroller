ORG 0000H
MOV 20H,#01H
MOV 21H,#10H
MOV 22H,#20H
MOV 23H,#0F0H
MOV 24H,#05H
MOV 25H,#09
MOV 26H,#084H
MOV 27H,#80H
MOV 28H,#0FFH
MOV 29H,#45H
MOV R0,#20H ; STARTING ADDRESS OF DATA 
MOV R7,#0AH ; COUNT 
CLR A ; CLEAR A
BACK: MOV B,@R0 ; GET FIRST DATA IN B 
CJNE A,B,NEXT ; COMPARE A AND B
NEXT : JNC SKIP 
MOV A,B ; EXCHANGE THE NUMBER 
SKIP : INC R0
DJNZ R7,BACK
MOV 30H,A
HERE : SJMP HERE

END 
